@using IP3_Group4.Models
@{
    ViewBag.Title = "ShopPie";
    Analytics analytics = ViewBag.Analytics;
}

@{
    var myChart = new Chart(width: 600, height: 400)
        .AddTitle("Shops")
        .AddSeries(
            name: "Shop",
            xValue: analytics.Shops.Select(s => s.Shop).ToArray(),
            yValues: analytics.Shops.Select(s => s.Visits).ToArray())
        .Write();
}